{
  "name": "Jelting plugin",
  "version": "1.0",
  "manifest_version": 2,
  "description":"This is jelting plugin",
  "icons": {"16": "jelly.png", "48": "jelly.png", "128": "jelly.png"},
  "permissions": [
	"tabs", 
	"http://*/*", 
	"https://*/*",
    "notifications"
  ],
  "options_page": "options.html",
  "background": { "scripts": ["jquery.js", "background.js"] },
  "browser_action": {
	"default_icon": "jelly.png",
    "default_popup": "popup.html"
  }
}
